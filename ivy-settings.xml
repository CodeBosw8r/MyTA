<ivysettings>
  <properties file="${ivy.settings.dir}/ivysettings-file.properties" />
  <property name="repo.maven.org" value="http://repo1.maven.org/maven2/" />
  <property name="dist.wso2.org" value="http://dist.wso2.org/maven2/" />
  <property name="maven2.pattern.ext" value="${maven2.pattern}.[ext]" />
  <property name="maven2.pattern" value="[organisation]/[module]/[revision]/[module]-[revision]" />
  <settings defaultResolver="default" />
  <include url="${ivy.default.conf.dir}/ivyconf-local.xml"/>
  <resolvers>
    <ibiblio name="maven2" root="${repo.maven.org}" pattern="${maven2.pattern.ext}" m2compatible="true" />
    <ibiblio name="wso2" root="${dist.wso2.org}" pattern="${maven2.pattern.ext}" m2compatible="true" />
    <ibiblio name="jahia" root="http://maven.jahia.org/maven2/" pattern="${maven2.pattern.ext}" m2compatible="true"/>
    <chain name="default" dual="true">
      <resolver ref="local" />
      <resolver ref="maven2" />
      <resolver ref="wso2" />
    </chain>
  </resolvers>
</ivysettings>