    <dependency org="javax.servlet" name="servlet-api" conf="build->default" rev="2.5" />
    <dependency org="javax.persistence" name="persistence-api" rev="1.0" />
    <dependency org="log4j" name="log4j" rev="1.2.14" />
    <dependency org="org.codehaus.jackson" name="jackson-mapper-asl" rev="1.9.2" />
    
  </dependencies>

</ivy-module> 